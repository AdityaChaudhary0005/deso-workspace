{
  "root": "apps/deso-react-template",
  "sourceRoot": "apps/deso-react-template/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/deso-react-template",
        "main": "apps/deso-react-template/src/index.ts",
        "tsConfig": "apps/deso-react-template/tsconfig.app.json",
        "assets": ["apps/deso-react-template/*.md"]
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "deso-react-template:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/deso-react-template/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/deso-react-template"],
      "options": {
        "jestConfig": "apps/deso-react-template/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
